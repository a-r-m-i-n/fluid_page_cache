{
    "name": "t3/fluid_page_cache",
    "type": "typo3-cms-extension",
    "description": "Creates automatically tags for TYPO3's page cache, based on used variables in rendered Fluid templates on current page.",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Armin Vieweg",
            "role": "Developer",
            "email": "info@v.ieweg.de",
            "homepage": "https://v.ieweg.de"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "typo3/cms-core": "^10.4.6 || ^11.5"
    },
    "autoload": {
        "psr-4": {
            "T3\\FluidPageCache\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "fluid_page_cache"
        }
    }
}
